<!doctype html>
<html lang="en" class="fuelux">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>{{ title }}</title>
    <link rel="shortcut icon" type="image/png" href="data:image/png;base64," />
	<link rel="stylesheet" type="text/css" href="/css/styles.css" />
	<link rel="stylesheet" type="text/css" href="https://fast.fonts.net/t/1.css?apiType=css&projectid=b6b2e2cc-768b-4ab8-90c7-e7f70174aa96"/>
    {{#if compiled}}
        <script src="{{ ui.staticBase }}js/main.js"></script>
    {{else}}
        <script data-main="{{ ui.staticBase }}js/main.js" src="{{ ui.staticBase }}../bower_components/requirejs/require.js"></script>
    {{/if}}
	<script>
        // this token needs to be sent to the server on any request other than GET.
        // basically any route that modifies data needs this token to ensure it's from this app
        // header name should be "_csrf"
        // Example of Backbone integration can be seen here --> https://gist.github.com/vernak2539/8337545
        window.app = {};
        window.app.csrfToken = '{{ csrfToken }}';
        window.app.serverUIConfig = {{{ ui }}};
        window.app.mid = '{{ mid }}';
        window.app.eid = '{{ eid }}';
        define( 'csrf-token', '{{ csrfToken }}' );
        define( 'server-ui-config', {{{ ui }}} );
        define( 'appId', '{{{ appId }}}');
	</script>
</head>
<body>
	<!--[if lt IE 7]>
